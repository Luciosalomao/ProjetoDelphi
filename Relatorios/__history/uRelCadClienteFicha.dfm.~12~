  object qryClientes: TZQuery
    Connection = dmConexao.conexaoDB
    Active = True
    SQL.Strings = (
      'select '
      'clienteId,'
      'nome,'
      'endereco,'
      'cidade,'
      'bairro,'
      'cep,'
      'telefone,'
      'email,'
      'dataDeNascimento'
      'from clientes order by  nome')
    Params = <>
    Left = 672
    Top = 224
